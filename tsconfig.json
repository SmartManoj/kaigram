{
  "compilerOptions": {
    "outDir": "./dist/js",
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": true,
    "jsx": "react",
    "jsxFactory": "h",
    // "baseUrl": ".",
    "noUnusedLocals": true
  },
  "include": ["./src/**/*.tsx", "./src/**/*.ts"],
  "vscode": {
    "rewriteTsconfig": true
  }
}
